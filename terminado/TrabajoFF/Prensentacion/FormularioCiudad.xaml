<Window x:Class="Prensentacion.FormularioCiudad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prensentacion"
        mc:Ignorable="d"
        Title="FormularioCiudad" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="DgCiudad" HorizontalAlignment="Left" Height="150" Margin="22,259,0,0" VerticalAlignment="Top" Width="726"/>
        <Button x:Name="btnciudad" Content="Registrar Ciudad" HorizontalAlignment="Left" Margin="109,202,0,0" VerticalAlignment="Top" Width="146" Height="34" Click="btnciudad_Click_1"/>
        <TextBox x:Name="txtNombreciudad" HorizontalAlignment="Left" Height="33" Margin="109,99,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="132"/>
        <TextBox x:Name="txtprecio" HorizontalAlignment="Left" Height="34" Margin="109,151,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="132"/>
        <Label Content="Nombre ciudad" HorizontalAlignment="Left" Margin="11,106,0,0" VerticalAlignment="Top"/>
        <Label Content="Precio" HorizontalAlignment="Left" Margin="22,159,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
